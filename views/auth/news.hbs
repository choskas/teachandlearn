 
<div id="news-header" >
  <img src="https://i.ibb.co/LtFGqF0/teachandlearn1.png"  />
</div>
 
 <div class="container">

   <div class="the-space">
    <h2>Materias</h2> 
    </div>
     
    <div class="top-themes">

        {{#each subjects}}
  <div class="card mb-3" style="width: 480px;" >
    <div class="row no-gutters">
      <div class="col-md-4">
        <img src="{{this.img}}" class="card-img" alt="...">
      </div>
      <div class="col-md-3">
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
          <p class="card-text">{{this.themes}}</p>
          <p class="card-text"><small class="text-muted">{{this.difficulty}}</small></p>
        </div>
      </div>
    </div>
  </div>

{{/each}}


</div>

<div class=subject-buttons>
    <a href="/viewAllSubjects" class="btn btn-primary">VER MÁS</a>
 
  </div>

    <div class="container">
      <h2>Maestros </h2>
      <div class="top-cont">
       
      {{#each users as |user|}}
      <div class="card mb-3" style="width: 480px;">
        <div class="row no-gutters">
          <div class="col-md-4">
            <img src="{{user.profile.img}}" class="card-img" alt="aquideberianestarlasfotosxdd">
          </div>
          <div class="col-md-3">
            <div class="card-body">
              <h5 class="card-title">{{user.userName}}</h5>
              <p class="card-text">{{user.email}}</p>
              <p class="card-text"><small class="text-muted">{{user.role}}</small></p>
            </div>
          </div>
        </div>
      </div>
      
      {{/each}}
      
    </div>
    </div>
    <div class=subject-buttons>
    <a href="/viewAllTeachers" class="btn btn-primary">VER MÁS</a>

    </div>


    <div class="container">
      <h2>Sesiones de estudio</h2>
      <div class="top-cont">
      {{#each meetings}}
      <div class="card mb-3" style="width: 480px;">
        <div class="row no-gutters">
          <div class="col-md-4">
            <img src="{{this.img}}" class="card-img" alt="aquideberianestarlasimagenesxddd">
          </div>
          <div class="col-md-3">
            <div class="card-body">
              <h5 class="card-title">{{this.name}}</h5>
              <p class="card-text">{{this.description}}</p>
              <p class="card-text"><small class="text-muted">{{this.date}}</small></p>
            </div>
          </div>
        </div>
      </div>
      
      {{/each}}
      </div>
      </div>
      <div class=subject-buttons>
      <a href="/viewAllMeetings" class="btn btn-primary">VER MÁS</a>
      </div>

 
     

      <div class="container">
        <h2>Grupos de estudio </h2>
        <div class="top-cont">
        {{#each grupos}}
        <div class="card mb-3" style="width: 480px;">
          <div class="row no-gutters">
            <div class="col-md-4">
              <img src="{{this.img}}" class="card-img" alt="aquideberiaestarlafotoxd">
            </div>
            <div class="col-md-3">
              <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.themes}}</p>
                <p class="card-text"><small class="text-muted">{{this.difficulty}}</small></p>
              </div>
            </div>
          </div>
        </div>
        
        {{/each}}
        </div>
        <div class=subject-buttons>
          <a href="/viewAllGroups" class="btn btn-primary">Explore Groups</a>
          <a href="/newGroup" class="btn btn-primary">Crear grupo de estudio</a>
        </div>

      </div>
<div id="teach-foot">
  <img src="https://i.ibb.co/1qTkNd8/bottom-news.png " />
</div>


      </div>